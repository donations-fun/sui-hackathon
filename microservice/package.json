{
  "name": "donations.fun-sui-microservice",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build common && nest build",
    "build:all": "nest build common && nest build donate-processor",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "watch": "nest build common && nest build donate-processor & nest start --watch",
    "start:debug": "nest build common && nest build donate-processor & nest start --watch --debug",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "dotenv -e .env.test -- jest --config ./apps/donate-processor/test/jest-e2e.json --force-exit",
    "migrate": "prisma migrate dev",
    "generate": "prisma generate",
    "deploy": "prisma migrate deploy",
    "test:migrate": "dotenv -e .env.test -- prisma migrate deploy"
  },
  "dependencies": {
    "@flowx-finance/sdk": "^1.10.16",
    "@mysten/sui": "^1.17.0",
    "@nestjs/common": "^10.3.9",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.9",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.3.9",
    "@nestjs/platform-express": "^10.3.9",
    "@nestjs/schedule": "^4.0.2",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/throttler": "^6.2.1",
    "@prisma/client": "^5.22.0",
    "agentkeepalive": "^4.5.0",
    "axios": "^1.8.2",
    "bignumber.js": "9.2.1",
    "cache-manager": "^5.6.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cron": "^3.1.7",
    "js-yaml": "^4.1.0",
    "lru-cache": "^8.0.5",
    "module-alias": "^2.2.3",
    "nest-winston": "^1.10.0",
    "openapi-client-axios": "^7.5.5",
    "rimraf": "^5.0.7",
    "rxjs": "^7.8.1",
    "twitter-api-v2": "^1.18.0",
    "web3": "^4.12.1",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.5.0",
    "@mysten/sui": "^1.17.0",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.9",
    "@types/cache-manager": "^4.0.6",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.16.5",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "prettier": "^3.3.2",
    "prisma": "^5.22.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.4",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "ts-proto": "^1.178.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  },
  "_moduleAliases": {
    "@monorepo/common": "dist/libs/common"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@monorepo/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@monorepo/common": "<rootDir>/libs/common"
    }
  },
  "engines": {
    "node": "20.x"
  }
}
